{
  "name": "react-vite-tooling-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.3.1",
  "scripts": {
    "start": "vite",
    "test": "yarn tsc && yarn lint && yarn format:check",
    "build": "yarn test && vite build",
    "serve": "vite preview",
    "tsc": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" \".eslintrc\"",
    "format:check": "prettier --check \"./**/*.{js,jsx,ts,tsx,css,md,json}\" \".eslintrc\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.2",
    "typescript": "^4.8.4",
    "vite": "^5.3.2",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
